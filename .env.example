APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://127.0.0.1:9802

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Force currency for testing/simulation (overrides IP-based detection)
# Set to CAD or USD to simulate pricing for that currency
# Leave empty or remove to use automatic IP-based detection
# FORCE_CURRENCY=CAD

# Order Sheet Configuration
# ORDER_SHEET_CURRENCY: Currency to use for order sheet pricing (default: USD)
ORDER_SHEET_CURRENCY=USD
# ORDER_SHEET_USE_LOCATION_PRICING: Use location-based pricing for order sheets (default: false)
ORDER_SHEET_USE_LOCATION_PRICING=false

# Checkout Configuration
# Tax rates and shipping fees for different brands/portals
# Each brand has its own tax rate based on jurisdiction:
# - Pinksky (USD): 5% tax rate
# - CCC Portal (CAD): 3% tax rate
# - Professional (CAD): 13% tax rate (HST in some Canadian provinces)
CHECKOUT_TAX_RATE_PINKSKY=5
CHECKOUT_TAX_RATE_CCCPORTAL=3
CHECKOUT_TAX_RATE_PROFESSIONAL=13
CHECKOUT_SHIPPING_FEE=60.00
CHECKOUT_TAX_RATE=13
